<h1> Create Transaction </h1>

<%= form_for(:transaction, :url => {:action => 'create'}) do |f| %>

  <label for="transaction_account_id">account id</label>
  <%= f.text_field(:account_id) %><br />

  <label for="transaction_cashFlow">cash flow</label>
  <%= f.text_field(:cashFlow) %><br />

  <label for="transaction_note">note</label>
  <%= f.text_field(:note) %><br />

  <%= submit_tag("Create Transaction") %>

<% end %>